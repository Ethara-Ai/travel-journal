{
  "name": "travel-journal-react",
  "version": "1.0.0",
  "description": "A beautiful, modern travel journal application built with React and Tailwind CSS. Track your adventures, rate destinations, and create a visual diary of your travels.",
  "type": "module",
  "author": "Travel Journal Team",
  "license": "MIT",
  "keywords": [
    "travel",
    "journal",
    "react",
    "tailwindcss",
    "vite",
    "trip-tracker",
    "travel-diary",
    "destination-tracker"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Ethara-AI/travel-journal-react.git"
  },
  "bugs": {
    "url": "https://github.com/Ethara-AI/travel-journal-react/issues"
  },
  "homepage": "https://github.com/Ethara-AI/travel-journal-react#readme",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules/.vite",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:verbose": "vitest run --reporter=verbose"
  },
  "dependencies": {
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^17.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "vite": "^7.3.1",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
